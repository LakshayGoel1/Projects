	public class Calculator2 {
    int result,result2,result3,result4;
    
    
    public int Addition(int a, int b){
        result=a + b;
        return result;
    }
    
    
    public int Subtraction(int a , int b){
        result2 = a - b;
       return result2;
    }
    
    public int Division(int a , int b){
        result3 = a/b;
       return result2;
    }
    
    public int Multiplication(int a , int b){
        result4 = a*b;
       return result4;
    }
    
   
}


